    "def f1(d):\n",
    "    re = 0\n",
    "    for i in d:\n",
    "        re += i\n",
    "    return re\n",
    "\n",
    "def f2(d):\n",
    "    re = 1\n",
    "    for i in d:\n",
    "        re *= i\n",
    "    return re\n",
    "\n",
    "def f3(d):\n",
    "    re = []\n",
    "    while d > 0:\n",
    "        re.append(d%10)\n",
    "        d = d//10\n",
    "    return re\n",
    "l = f3(1234)\n",
    "print(sum(l) + 2*f1(l) + 3*f2(l))\n"
   ]
  }
